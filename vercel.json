{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/utilisateur/(.*)",
      "dest": "/src/index.ts"
    },
    {
      "src": "/test/(.*)",
      "dest": "/src/index.ts"
    },
    {
      "src": "/message/(.*)",
      "dest": "/src/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ],
  "env": {
    "PORT": 4000
  }
}
